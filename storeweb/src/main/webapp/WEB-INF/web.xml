<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  	<param-name>contextConfigLocation</param-name>
  	<param-value>classpath:/beans.xml</param-value>
  </context-param>
  <listener>
  	<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  
  <servlet>
  	<servlet-name>CreateStoreServlet</servlet-name>
  	<display-name>CreateStoreServlet</display-name>
  	<description></description>
  	<servlet-class>com.rohant.store.servlet.CreateStoreServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayStoreDetailServlet</servlet-name>
  	<display-name>DisplayStoreDetailServlet</display-name>
  	<description></description>
  	<servlet-class>com.rohant.store.servlet.DisplayStoreDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateStoreServlet</servlet-name>
  	<url-pattern>/CreateStoreServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayStoreDetailServlet</servlet-name>
  	<url-pattern>/DisplayStoreDetailServlet</url-pattern>
  </servlet-mapping>
</web-app>
